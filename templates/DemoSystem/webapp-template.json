{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "prefix": {
            "maxLength":11,
            "type": "string"
        },
        "userObjectId":{
            "type": "string"
        },
        "storageaccountkeyvaulturl":{
            "type": "string"
        },
        "storageaccountconnectionstring":{
            "type": "string"
        }
    },
    "variables": {
        "hostingname":"[concat(parameters('prefix'),'-hostingplan')]",
        "websitename":"[concat(parameters('prefix'),'appservice')]"
    },
    "resources": [
        {
            "type": "Microsoft.Web/serverfarms",
            "apiVersion": "2018-02-01",
            "name":"[variables('hostingname')]",
            "location":"[resourceGroup().location]",
            "sku": {
                "tier": "Shared",
                "name":"S1"
            },
            "properties": {

            }
        },
        {
            "type": "Microsoft.Web/sites",
            "apiVersion": "2018-11-01",
            "name":"[variables('websitename')]",
            "location":"[resourceGroup().location]",
            "properties": {
                "serverFarmId":"[resourceId(resourceGroup().name,'Microsoft.Web/serverFarms', variables('hostingname'))]"
                
           },
           "dependsOn": [
               "[resourceId(resourceGroup().name,'Microsoft.Web/serverFarms', variables('hostingname'))]"
           ]
        }
    ],
    "outputs": {

    }
    }